{
  "name": "nestjs-webp-converter",
  "version": "0.0.1",
  "description": "NestJS module to convert images to webp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "author": {
    "name": "Pedro de Mattos Mariano Mendes Neto",
    "email": "pedromneto97@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pedromneto97/nestjs-webp-converter"
  },
  "keywords": [
    "nestjs",
    "webp",
    "image",
    "converter"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "0.2.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": false
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}